{
  "name": "gomoku-online",
  "version": "1.0.0",
  "description": "multiplayer game",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm install && npm run build"
  },
  "dependencies": {
    "concurrently": "^5.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.5.0",
    "nodemon": "^2.0.2",
    "socket.io": "^2.3.0"
  }
}
